  <JobDef>
    <defName>YAMP_CarryToMedPod</defName>
    <driverClass>YAMP.JobDriver_CarryToMedPod</driverClass>
    <reportString>carrying TargetA to med pod.</reportString>
    <casualInterruptible>false</casualInterruptible>
  </JobDef>
</Defs>
